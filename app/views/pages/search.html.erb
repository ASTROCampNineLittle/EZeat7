<%# banner %>
<div class="w-full h-auto mt-0 bg-blue-200">
  <div class="items-center m-0">
    <!-- component -->
    <div class="grid h-96 items-center w-9/12 ml-32 ">
      <div class="w-11/12 p-8 bg-white sm:w-8/12 md:w-1/2 lg:w-5/12 rounded">
        <h1 class="text-lg font-semibold">餐廳名稱搜尋</h1>
        <form class="mt-6">
          <label class="block mt-2 text-xs font-semibold text-gray-600 uppercase">
            餐廳名稱
            <input placeholder="請輸入餐廳名稱" class="block w-full p-2 mt-2 text-gray-700 bg-gray-200 appearance-none focus:outline-none focus:bg-gray-300 focus:shadow-inner rounded" required />
          </label>
          <label type="date" class="block mt-2 text-xs font-semibold text-gray-600 uppercase">
              日期
              <input placeholder="2021/12/25" class="block w-full p-2 mt-2 text-gray-700 bg-gray-200 appearance-none focus:outline-none focus:bg-gray-300 focus:shadow-inner rounded" required />
          </label>
          <div class="flex justify-between">
            <label type="time" class="block mt-2 text-xs font-semibold text-gray-600 uppercase">
              時間
              <input placeholder="19:00" class="block w-full p-2 mt-2 text-gray-700 bg-gray-200 appearance-none focus:outline-none focus:bg-gray-300 focus:shadow-inner rounded" required />
            </label>
            <label type="number" class="block mt-2 text-xs font-semibold text-gray-600 uppercase w-2/5">
              人數
              <input name="password-confirm" placeholder="2" autocomplete="new-password" class="block w-full p-2 mt-2 text-gray-700 bg-gray-200 appearance-none focus:outline-none focus:bg-gray-300 focus:shadow-inner rounded" required />
            </label>
          </div>
          <button type="submit" class="w-full py-2 mt-6 font-medium tracking-widest text-white uppercase bg-yellow-500 shadow-lg focus:outline-none hover:bg-yellow-600 hover:shadow-none rounded-lg">
            搜尋
          </button>
        </form>
      </div>
  </div>
  </div>
</div>



